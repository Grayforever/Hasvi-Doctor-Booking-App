<layer-list xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dp"
                android:color="@color/mtrl_textinput_default_box_stroke_color"
                tools:ignore="PrivateResource" />
            <corners
                android:bottomRightRadius ="@dimen/mtrl_textinput_box_corner_radius_medium"
                android:bottomLeftRadius="@dimen/mtrl_textinput_box_corner_radius_medium"
                android:topLeftRadius ="@dimen/mtrl_textinput_box_corner_radius_medium"
                android:topRightRadius="@dimen/mtrl_textinput_box_corner_radius_medium"/>
        </shape>
    </item>
</layer-list>